package org.usfirst.frc.team6317.robot.commands;

import org.usfirst.frc.team6317.robot.Robot;

import edu.wpi.first.wpilibj.command.CommandGroup;

public class TestAuto extends CommandGroup {
	public TestAuto() {
		addSequential(new DriveFor(35.82, 0.2, 0.7));
		addSequential(new TurnFor("L", 24));
		addSequential(new DriveFor(36, 0.2, 0.87));
		addSequential(new TurnFor("R", 24));
		addSequential(new DriveFor(70, 0.2, 0.87));
		//addSequential(new DriveUntil(30, 0.2));
	}
}
